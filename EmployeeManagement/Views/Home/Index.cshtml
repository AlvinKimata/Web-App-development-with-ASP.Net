@model IEnumerable<EmployeeManagement.Models.Employee>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Employee List";
 }
<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>name</th>
            <th>Department</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.Id</td>
                <td>@employee.Name</td>
                <td>@employee.Deprtment</td>
            </tr>
        }
    </tbody>
        
</table>
